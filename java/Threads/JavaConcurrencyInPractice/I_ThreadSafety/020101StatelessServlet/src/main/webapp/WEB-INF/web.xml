<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Stateless Servlet</display-name>

  <welcome-file-list>
      <welcome-file>index.html</welcome-file>
      <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
      <servlet-name>StatelessFactorizer</servlet-name>
      <servlet-class>mx.naui.concurrency.StatelessFactorizer</servlet-class>
  </servlet>

  <servlet-mapping>
      <servlet-name>StatelessFactorizer</servlet-name>
      <url-pattern>/sf</url-pattern>
  </servlet-mapping>
</web-app>
